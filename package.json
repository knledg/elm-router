{
  "name": "elm-router",
  "version": "2.0.1",
  "description": "A router for launching Elm apps",
  "main": "lib/js/elm-router.js",
  "scripts": {
    "build": "node_modules/babel-cli/bin/babel.js  src/ --out-dir lib/js/",
    "test": "npm run js-test && npm run elm-test",
    "js-test": "jest",
    "elm-test": "cd test/elm && ../../node_modules/elm-test/bin/elm-test",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knledg/elm-router.git"
  },
  "bin": {
    "elm-router": "bin/elm-router"
  },
  "keywords": [
    "Elm",
    "router"
  ],
  "author": "Consolidated Knowledge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knledg/elm-router/issues"
  },
  "homepage": "https://github.com/knledg/elm-router#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "elm-test": "^0.18.2",
    "jest": "^18.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "testRegex": "/test/js/.*\\.test\\.js$"
  }
}
